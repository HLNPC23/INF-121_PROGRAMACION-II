class Departamento:
  def __init__(self,nombre,area):
    self.nombre=nombre
    self.area=area
    self.empleados=[]

  def agregar_empleado(self,empleado):
    self.empleados.append(empleado)

  def mostrar_empleados(self):
    print(f"Empleados del departamento {self.nombre}:")
    for empleado in self.empleados:
      print(f"- {empleado.nombre} (empleado.cargo)") 
  
  def cambio_salario(self, porcentaje):
    for empleado in self.empleados:
      nuevo_salario = empleado.salario * (1 + porcentaje / 100)

  def empleadosCompartidos(self, otro):
    for empleado in self.empleados:
      if empleado in otro.empleados:
        return True
    return False
class Empleado:
  def __init__(self,nombre,cargo, sueldo):
    self.nombre=nombre
    self.cargo=cargo
    self.salario=sueldo


