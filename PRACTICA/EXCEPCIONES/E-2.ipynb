class NumeroInvalidoException(Exception):
    pass
class Calculadora:
    @staticmethod
    def sumar(a, b):
        return a + b
    
    @staticmethod
    def restar(a, b):
        return a - b
    
    @staticmethod
    def multiplicar(a, b):
        return a * b
    @staticmethod
    def dividir(a, b):
        if b == 0:
            raise ArithmeticError("Error: No se puede dividir por cero")
        return a / b
    @staticmethod
    def convertir_a_entero(texto):
        try:
            return int(texto)
        except ValueError:
            raise NumeroInvalidoException(f"Error: '{texto}' no es un número válido")
def main():
    print("=== CALCULADORA ===\n")
    print("--- PRUEBAS EXITOSAS ---")
    try:
        print(f"Suma: 5 + 3 = {Calculadora.sumar(5, 3)}")
        print(f"Resta: 10 - 4 = {Calculadora.restar(10, 4)}")
        print(f"Multiplicación: 6 * 7 = {Calculadora.multiplicar(6, 7)}")
        print(f"División: 15 / 3 = {Calculadora.dividir(15, 3)}")
        
        numero = Calculadora.convertir_a_entero("42")
        print(f"Conversión: '42' -> {numero}")
    except Exception as e:
        print(f"Error inesperado: {e}")
    print("\n--- PRUEBAS CON ERRORES ---")
    try:
        resultado = Calculadora.dividir(10, 0)
        print(f"División: 10 / 0 = {resultado}")
    except ArithmeticError as e:
        print(f"{e}")
    try:
        numero = Calculadora.convertir_a_entero("abc")
        print(f"Conversión: 'abc' -> {numero}")
    except NumeroInvalidoException as e:
        print(f"{e}")
    try:
        numero = Calculadora.convertir_a_entero("12.5")
        print(f"Conversión: '12.5' -> {numero}")
    except NumeroInvalidoException as e:
        print(f"{e}")
    try:
        numero = Calculadora.convertir_a_entero("100")
        print(f"Conversión: '100' -> {numero}")
    except NumeroInvalidoException as e:
        print(f"{e}")
if __name__ == "__main__":
    main()